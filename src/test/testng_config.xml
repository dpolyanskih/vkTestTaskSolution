<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite configfailurepolicy="continue" name="vkTestTask" parallel="methods" thread-count="1" verbose="2" preserve-order="false">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
    </listeners>
    <test name="likes.add" verbose="2">
        <groups>
            <run>
                <include name="likes.add"/>
            </run>
        </groups>
        <packages>
            <package name="ru.polyanskih.tests.likes.*"/>
        </packages>
    </test>
    <test name="likes.delete" verbose="2">
        <groups>
            <run>
                <include name="likes.delete"/>
            </run>
        </groups>
        <packages>
            <package name="ru.polyanskih.tests.likes.*"/>
        </packages>
    </test>
    <test name="likes.getList" verbose="2">
        <groups>
            <run>
                <include name="likes.getList"/>
            </run>
        </groups>
        <packages>
            <package name="ru.polyanskih.tests.likes.*"/>
        </packages>
    </test>
    <test name="likes.isLiked" verbose="2">
        <groups>
            <run>
                <include name="likes.isLiked"/>
            </run>
        </groups>
        <packages>
            <package name="ru.polyanskih.tests.likes.*"/>
        </packages>
    </test>
</suite>
